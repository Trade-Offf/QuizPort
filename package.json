{
  "name": "quizport",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:css": "tailwindcss -i ./src/app/globals.css -o ./src/app/tw.gen.css --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "tailwindcss -i ./src/app/globals.css -o ./src/app/tw.gen.css --minify",
    "cf:build": "NPM_CONFIG_CACHE=.npm-cache NEXT_DISABLE_LIGHTNINGCSS=1 LIGHTNINGCSS_FORCE_WASM=1 NEXT_OTEL_DISABLED=1 npx open-next@3.1.0 build --target=cloudflare-pages",
    "cf:dev": "npx wrangler pages dev .open-next --compatibility-flag=nodejs_compat",
    "cf:deploy": "npx wrangler pages deploy .open-next --branch=main"
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@heroui/theme": "^2.4.23",
    "@prisma/client": "^6.18.0",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.5",
    "@wagmi/connectors": "^5.1.7",
    "@wagmi/core": "^2.11.7",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.24",
    "lightningcss": "^1.28.0",
    "next": "15.1.4",
    "next-auth": "^4.24.7",
    "nodemailer": "^7.0.10",
    "prisma": "^6.18.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "^2.3.2",
    "three": "^0.180.0",
    "vercel": "^37.0.0",
    "viem": "^2.21.3",
    "wagmi": "^2.12.10",
    "zod": "^3.23.8"
  },
