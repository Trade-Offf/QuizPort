    "@types/node": "^20.16.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.1.4",
    "lightningcss-wasm": "^1.28.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "wrangler": "^3.76.0"
  },
  "overrides": {
    "lightningcss": "^1.28.0"
  }
